{
    "device_ip": "192.168.1.10",
    "username": "admin",
    "password": "your-password-here",
    "enable_password": "your-enable-password-here",
    "ssh_port": 22,
    "device_type": "controller",
    "interface_name": "gigabitethernet0/0/1",
    "key_algorithm": "aes-256-gcm",
    "simulation_mode": true,
    "operation_delay": 2.0,
    "default_interface": "gigabitethernet0/0/1",
    "default_algorithm": "aes-256-gcm",
    "default_priority": "high",
    "rollback_on_failure": true,
    "advance_warning_seconds": 30,
    "cleanup_delay_seconds": 60,
    "validate_key_before_rotation": true,
    "validate_device_after_rotation": true,
    "notification_url": "https://webhook.example.com/aos8-status",
    "notification_headers": {
        "Authorization": "Bearer your-token-here",
        "Content-Type": "application/json",
        "X-Device-Type": "aos8-controller"
    },
    "custom_metadata": {
        "environment": "production",
        "team": "network-security",
        "project": "sae-client",
        "device_model": "Aruba-7210",
        "location": "datacenter-1",
        "vrf": "default",
        "management_vlan": "100"
    },
    "supported_algorithms": ["aes-256-gcm", "aes-128-gcm", "chacha20-poly1305"],
    "supported_device_types": ["controller", "ap", "switch"],
    "supported_interfaces": [
        "eth0", 
        "eth1", 
        "gigabitethernet0/0/1", 
        "gigabitethernet0/0/2",
        "gigabitethernet0/0/3",
        "gigabitethernet0/0/4"
    ],
    "aos8_versions": ["8.6.0.0", "8.7.0.0", "8.8.0.0", "8.9.0.0", "8.10.0.0"],
    "max_key_slots": 16,
    "key_lifetime_hours": 24,
    "debug_mode": true,
    "ssh_timeout": 30,
    "command_timeout": 60,
    "retry_attempts": 3,
    "retry_delay": 5,
    "enable_rollback": true,
    "enable_notifications": true,
    "enable_validation": true,
    "enable_simulation": true,
    "log_level": "INFO",
    "log_file": "/var/log/aos8-persona.log",
    "backup_config": true,
    "backup_location": "/backup/aos8-configs/",
    "monitoring_enabled": true,
    "monitoring_interval": 300,
    "alert_threshold": 0.8,
    "health_check_enabled": true,
    "health_check_interval": 60
}
